{
  "name": "mysql-report-runner",
  "version": "1.0.0",
  "description": "Web application to run MySQL reports with Express and Vite",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node server/index.js"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "^4.0.3",
    "fs-extra": "^11.1.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "node-forge": "^1.3.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "mysql",
    "express",
    "vite",
    "reports"
  ],
  "author": "",
  "license": "MIT"
}
